{
  "moz:firefoxOptions": {
    "prefs": {
      "dom.webgpu.enabled": true
    },
    "args": []
  },
  "goog:chromeOptions": {
    "args": [
      "--enable-unsafe-webgpu",
      "--enable-features=Vulkan,VulkanFromANGLE",
      "--disable-vulkan-fallback-to-gl-for-testing",
      "--disable-vulkan-surface",
      "--disable-features=VaapiVideoDecoder",
      "--ignore-gpu-blocklist",
      "--use-angle=vulkan"
    ]
  }
}